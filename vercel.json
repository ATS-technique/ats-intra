{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "installCommand": "corepack enable && corepack prepare pnpm@8.15.4 --activate && pnpm install",
  "buildCommand": "pnpm run build",
  "outputDirectory": "front/dist"
}
