{
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "front/dist" }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets|favicon\\.ico|robots\\.txt|manifest\\.json).*)",
      "destination": "/front/index.html"
    }
  ]
}